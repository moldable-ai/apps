{
  "name": "moldable-apps",
  "version": "1.0.0",
  "description": "Official Moldable apps collection",
  "type": "module",
  "private": true,
  "scripts": {
    "generate-manifest": "node scripts/generate-manifest.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write . --ignore-path .prettierignore --cache --cache-location .prettiercache",
    "format:check": "prettier --check . --ignore-path .prettierignore --cache --cache-location .prettiercache",
    "check-types": "node scripts/check-types.js",
    "pre-commit": "pnpm format && git add -u && pnpm lint && pnpm check-types",
    "prepare": "husky",
    "app:copy": "node scripts/copy-apps.js",
    "app:copy:all": "node scripts/copy-apps.js --all",
    "app:update": "node scripts/update-apps.js",
    "app:update:all": "node scripts/update-apps.js --all",
    "release": "node scripts/release.js",
    "release:dry": "node scripts/release.js --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moldable-ai/apps.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/node": "^22.15.21",
    "eslint": "^9.28.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.6.12",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1"
  },
  "packageManager": "pnpm@9.0.0"
}
